{
  "_version": "1.49.0",
  "sap.app": {
      "id": "zalcsa.aprobacionnotas",
      "type": "application",
      "i18n": "i18n/i18n.properties",
      "applicationVersion": {
          "version": "0.0.1"
      },
      "title": "{{appTitle}}",
      "description": "{{appDescription}}",
      "resources": "resources.json",
      "sourceTemplate": {
          "id": "@sap/generator-fiori:basic",
          "version": "1.10.1",
          "toolsId": "c58ec999-cbc8-4772-9c8a-d8f64209334c"
      },
      "dataSources": {
          "ZSO_FI_NOTA_CREDITO_APROBAR_SRV": {
              "uri": "/sap/opu/odata/sap/ZSO_FI_NOTA_CREDITO_APROBAR_SRV/",
              "type": "OData",
              "settings": {
                  "localUri": "localService/metadata.xml"
              }
          },
          "mainModel": {
            "uri": "/sap/opu/odata/sap/ZSO_FI_NOTA_CREDITO_APROBAR_SRV/",
            "type": "OData",
            "settings": {
                "localUri": "localService/metadata.xml"
            }
         }
      }
  },
  "sap.ui": {
      "technology": "UI5",
      "icons": {
          "icon": "",
          "favIcon": "",
          "phone": "",
          "phone@2": "",
          "tablet": "",
          "tablet@2": ""
      },
      "deviceTypes": {
          "desktop": true,
          "tablet": true,
          "phone": true
      }
  },
  "sap.ui5": {
      "flexEnabled": false,
      "dependencies": {
          "minUI5Version": "1.115.1",
          "libs": {
              "sap.ui.layout": {},
              "sap.m": {},
              "sap.ui.core": {},
              "sap.f": {},
              "sap.suite.ui.generic.template": {},
              "sap.ui.comp": {},
              "sap.ui.generic.app": {},
              "sap.ui.table": {},
              "sap.ushell": {}
          }
      },
      "contentDensities": {
          "compact": true,
          "cozy": true
      },
      "models": {
          "i18n": {
              "type": "sap.ui.model.resource.ResourceModel",
              "settings": {
                  "bundleName": "zalcsa.aprobacionnotas.i18n.i18n"
              }
          },
          "mainModel": {
            "dataSource": "mainModel",
            "type": "sap.ui.model.odata.v2.ODataModel",
            "settings": {
              "defaultOperationMode": "Server",
              "defaultBindingMode": "OneWay",
              "defaultCountMode": "Request"
            },
            "preload": true
        },
          "localModel": {
            "type": "sap.ui.model.json.JSONModel",
            "uri": "model/localModel.json"
          },
          "": {
              "type": "sap.ui.model.odata.v2.ODataModel",
              "settings": {
                  "defaultOperationMode": "Server",
                  "defaultBindingMode": "TwoWay",
                  "defaultCountMode": "Request",
                  "useBatch": false,
                  "metadataUrlParams": {
                      "sap-language": "ES"
                  }
              },              
              "dataSource": "ZSO_FI_NOTA_CREDITO_APROBAR_SRV",
              "preload": true
          }
      },
      "resources": {
          "css": [
              {
                  "uri": "css/style.css"
              }
          ]
      },
      "routing": {
          "config": {
              "routerClass": "sap.m.routing.Router",
              "viewType": "XML",
              "async": true,
              "viewPath": "zalcsa.aprobacionnotas.view",
              "controlAggregation": "pages",
              "controlId": "idFexibleColumnLayout",
              "clearControlAggregation": false
          },
          "routes": [
              {
                  "name": "TargetMain",
                  "pattern": "TargetMain",
                  "target": [
                      "TargetMain"
                  ]
              },
              {
                  "pattern": ":Layout:",
                  "name": "master",
                  "target": [
                      "master",
                      "detail"
                  ]
              },
              {
                  "pattern": "detail/{Layout}/{Documento}/{Sociedad}",
                  "name": "detail",
                  "target": [
                      "master",
                      "detail"
                  ]
              }
          ],
          "targets": {
              "TargetMain": {
                  "viewType": "XML",
                  "transition": "slide",
                  "clearControlAggregation": false,
                  "viewId": "Main",
                  "viewName": "Main"
              },
              "master": {
                  "viewName": "Master",
                  "controlAggregation": "beginColumnPages"
              },
              "detail": {
                  "viewName": "Detail",
                  "controlAggregation": "midColumnPages"
              }
          }
      },
      "rootView": {
          "viewName": "zalcsa.aprobacionnotas.view.Main",
          "type": "XML",
          "async": true,
          "id": "Main"
      }
  },
  "sap.cloud": {
      "public": true,
      "service": "zalcsaaprobacionnotas"
  }
}